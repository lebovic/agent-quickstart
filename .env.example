# Database
DATABASE_URL=postgresql://user:password@localhost:5432/agentquickstart

# Anthropic API
ANTHROPIC_API_URL=https://api.anthropic.com
# API key for "hosted" provider mode (server proxies requests with this key)
# PROXY_ANTHROPIC_API_KEY=sk-ant-...

# Docker session image
DEFAULT_SESSION_IMAGE=lebovic/agent-quickstart-sessions:latest

# Required secrets (min 44 chars for 256-bit entropy). Generate with: openssl rand -base64 32
JWT_SECRET=
ENCRYPTION_SECRET=
BETTER_AUTH_SECRET=

# Deploy URL (required in production for auth redirects, email links, etc.)
DEPLOY_URL=https://example.com

# BetterAuth URL (defaults to DEPLOY_URL, set to http://localhost:3000 for local dev)
BETTER_AUTH_URL=http://localhost:3000

# URL for executors (Docker/Modal) to reach the API (optional, defaults to DEPLOY_URL)
# In development, use a tunnel (ngrok, Cloudflare Tunnel) or host.docker.internal
# API_URL_FOR_EXECUTORS=http://host.docker.internal:3000

# Git integration mode ("required" | "optional" | "disabled")
# - required: Repo selection mandatory for sessions, GitHub App must be configured
# - optional: Repo selection available but not required (default)
# - disabled: No git features, works without GitHub App setup
GIT_INTEGRATION_MODE=optional
NEXT_PUBLIC_GIT_INTEGRATION_MODE=optional

# GitHub App (required when GIT_MODE is "required" or "optional")
# See docs/setting_up_github.md for setup instructions
# GITHUB_APP_ID=
# GITHUB_APP_CLIENT_ID=
# GITHUB_APP_CLIENT_SECRET=
# GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----"
# GITHUB_APP_SLUG=your-app-slug

# Email (optional - magic links logged to server console if not configured)
# RESEND_API_KEY=
# EMAIL_FROM_DOMAIN=auth.example.com

# Remote Docker host (optional - defaults to local socket)
# DOCKER_HOST=remote-docker.example.com
# DOCKER_PORT=2376
# DOCKER_CA_CERT_PATH=/path/to/ca.pem
# DOCKER_CLIENT_CERT_PATH=/path/to/cert.pem
# DOCKER_CLIENT_KEY_PATH=/path/to/key.pem

# Modal sandboxes (optional)
# MODAL_TOKEN_ID=
# MODAL_TOKEN_SECRET=
# MODAL_APP_NAME=agent-quickstart

# Default executor for new environments ("docker" or "modal")
# DEFAULT_EXECUTOR=docker

# Client-side accessible config (optional)
NEXT_PUBLIC_DEFAULT_GIT_REPO=owner/repo-name
